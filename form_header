input[type=submit], .button {
      -webkit-appearance: none;
      appearance: none;
      display:block;
      font-family:sans-serif;
      font-family:var(--font-family-sans,sans-serif);
      text-decoration:none;
      border:0;
      padding: 17px 25px;
      margin:10px auto;
      text-align: center;
      border-radius: 8px;
      background-color: rgba(var(--button-color-base,8,79,140),1);
      color: #fff;
      letter-spacing: 0px;
      text-shadow: none;
      font-size: 22px;
      line-height:1;
      font-weight: bold;
      cursor: pointer;
      opacity:1;
      min-width:245px;
      /*box-shadow: 0px 4px 15px 0px rgba(var(--button-color-base,8,79,140),0.4);*/
      transition-property: min-width, transform, border-radius;
      transition-duration: 0.0s;
      transition-timing-function: linear;
      transform-origin:center center;
      position:relative;
      overflow:hidden;
}
.button_active_fancy {
    pointer-events:none;
    transition-delay: 0.5s;
    transition-duration: 0.3s;
    padding:13px;
    min-width:0px;
    border-radius:100px;
    font-size:0;
}
.button_active_fancy img {
    display:none;
}
.button.button_active_fancy .loader {
    display:block;
}
.button_darken {
    filter:brightness(75%);
    pointer-events:none;
}
